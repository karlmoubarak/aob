{
  "kind": "singleType",
  "collectionName": "infos",
  "info": {
    "name": "Info"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "About": {
      "type": "richtext",
      "default": "The About Text",
      "required": true
    },
    "Contact": {
      "type": "richtext",
      "default": "The contact info",
      "required": true
    },
    "PrivacyPolicy": {
      "type": "richtext",
      "default": "The website's privacy policy",
      "required": true
    },
    "slug": {
      "type": "uid",
      "default": "about"
    },
    "UsageInstructions": {
      "type": "richtext",
      "default": "Usage instructions for the website",
      "required": true
    },
    "LinkToPublicCodeRepository": {
      "type": "string"
    }
  }
}
